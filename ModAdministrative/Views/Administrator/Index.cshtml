@model ModAdministrative.Models.tbuseradmin
@{
    ViewBag.Title = "Index";
    
}


<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>Login</title>

</head>
<body>
    <div id="login-div">
        @using (Html.BeginForm("Registrar", "Administrator",FormMethod.Post))
        {
            <table>
                <tr>
                    <td></td>
                    <td style="text-decoration: underline">Administrador</td>
                </tr>
                <tr>
                    <td>@Html.LabelFor(Model => Model.nameuseradmin)</td>
                    <td>@Html.EditorFor(Model => Model.nameuseradmin)</td>
                </tr>
                <tr>
                    <td></td>
                    <td>@Html.ValidationMessageFor(Model => Model.nameuseradmin)</td>
                </tr>
                <tr>
                    <td>@Html.LabelFor(Model => Model.emailuseradmin)</td>
                    <td>@Html.EditorFor(Model => Model.emailuseradmin)</td>
                </tr>
                <tr>
                    <td></td>
                    <td>@Html.ValidationMessageFor(Model => Model.emailuseradmin)</td>
                </tr>
                <tr>
                    <td>@Html.LabelFor(Model => Model.passworduseradmin)</td>
                    <td>@Html.EditorFor(Model => Model.passworduseradmin)</td>
                </tr>
                <tr>
                    <td></td>
                    <td>@Html.ValidationMessageFor(Model => Model.passworduseradmin)</td>
                </tr>
                <tr>
                    <td></td>
                    <td><input type="submit" name="Login" value="Login"/></td>
                    <td><input type="reset" name="Limpiar" value="Limpiar" /></td>
                </tr>
            </table>
        }
    </div>

</body>
</html>

